1) SELECT *FROM T_HOTEL;

2) SELECT *FROM T_HOTEL WHERE CITY='NEW YORK';

3) SELECT NAME,CITY FROM T_GUEST 
   ORDER BY CITY;

4) SELECT *FROM T_ROOM 
   WHERE TYPE='N'
   ORDER BY PRICE ASC;

5) SELECT COUNT(*) AS NUMBER_OF_HOTELS
   FROM T_HOTEL;

6) SELECT DISTINCT CITY FROM T_GUEST;

7) SELECT AVG(PRICE) AS AVERAGE_PRICE
   FROM T_ROOM;

8) SELECT H.NAME,R.ROOM_NO,R.TYPE
   FROM T_HOTEL H
   LEFT OUTER JOIN
   T_ROOM R
   ON(H.HOTEL_NO=R.HOTEL_NO)
   GROUP BY R.HOTEL_NO;

9) SELECT H.NAME, B.DATE_FROM,B.DATE_TO,B.ROOM_NO
   FROM T_HOTEL H
   INNER JOIN
   T_BOOKING B
   ON(H.HOTEL_NO=B.HOTEL_NO)
   WHERE H.CITY='NEW YORK';

10) SELECT COUNT(*) FROM T_BOOKING
    WHERE DATE_FROM = (SELECT EXTRACT(MONTH FROM DATE_FROM FROM T_BOOKING) AS MONTH WHERE MONTH='SEP');

11) SELECT G.NAME,G.CITY
    FROM T_GUEST G
    WHERE G.GUEST_NO=(SELECT EXTRACT(MONTH FROM DATE_FROM FROM T_BOOKING) AS MONTH WHERE MONTH='AUG' AND
                       H.HOTEL_NO=(SELECT H.HOTEL_NO FROM T_HOTEL WHERE H.CITY='NEW YORK'));

   

